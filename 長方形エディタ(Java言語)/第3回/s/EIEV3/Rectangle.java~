//package EIEV3;
import java.awt.Color;


class Rectangle{
    
    int x;
    int y;
    int height;
    int width;
    String color;
    
    Rectangle(int x,int y,int height,int width,String color){
        this.x=x;
        this.y=y;
        this.height=height;
        this.width=width;
	    this.color=color;

    }
}